---
layout: null
---

<rss version="2.0">
<channel>
 <title>{{ site.name }}</title>
 {% for post in site.posts %}
 <item>
   <title>{{ post.title | xml_escape }}</title>
   {% if post.podcast %}
   <enclosure type="audio/mpeg" url="{{ site.mp3url }}xpenguin-{{ post.date | date: '%Y-%m-%d' }}.mp3" />
   {% endif %}
 </item>
 {% endfor %}
</channel>
</rss>
