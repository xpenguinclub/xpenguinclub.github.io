---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>{{ site.name }}</title>
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   {% if post.podcast %}
   <link rel="enclosure" type="audio/mpeg" href="{{ site.mp3url }}xpenguin-{{ post.date | date: '%Y-%m-%d' }}.mp3" length="500" />
   {% endif %}
 </entry>
 {% endfor %}
</feed>
